<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="200.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scheduleCreator.view.CourseEditDialogController">
   <children>
      <GridPane layoutX="21.0" layoutY="35.0" prefHeight="162.0" prefWidth="394.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label layoutX="5.0" prefHeight="20.0" text="Preferred Professor" GridPane.rowIndex="3" />
            <Label layoutX="5.0" prefHeight="19.0" prefWidth="113.0" text="Course Department" />
            <Label layoutX="5.0" prefHeight="20.0" prefWidth="99.0" text="Course Number" GridPane.rowIndex="1" />
            <Label layoutX="5.0" prefHeight="20.0" prefWidth="82.0" text="Course Name" GridPane.rowIndex="2" />
            <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
              <buttons>
                <Button mnemonicParsing="false" onAction="#handleConfirm" text="Confirm" textAlignment="CENTER" />
                  <Button mnemonicParsing="false" onAction="#handleCancel" text="Cancel" textAlignment="CENTER" />
              </buttons>
            </ButtonBar>
            <TextField fx:id="courseDepartmentField" GridPane.columnIndex="1" />
            <TextField fx:id="courseNumberField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="courseNameField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="prefProfField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
