<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="140.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scheduleCreator.view.LoginDialogController">
   <children>
      <GridPane layoutX="17.0" layoutY="17.0" prefHeight="108.0" prefWidth="265.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="129.0" minWidth="0.0" prefWidth="85.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="267.0" minWidth="10.0" prefWidth="182.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="User:" />
            <Label text="Password" GridPane.rowIndex="1" />
            <PasswordField fx:id="passwordField" prefHeight="25.0" prefWidth="163.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="usernameField" GridPane.columnIndex="1" />
            <ButtonBar prefHeight="35.0" prefWidth="169.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <buttons>
                  <Button mnemonicParsing="false" onAction="#handleLogin" prefHeight="25.0" prefWidth="63.0" text="Log In" />
                  <Button mnemonicParsing="false" onAction="#handleCancel" prefHeight="25.0" prefWidth="116.0" text="Cancel" />
               </buttons>
            </ButtonBar>
         </children>
      </GridPane>
   </children>
</AnchorPane>
